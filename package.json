        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-vue": "^9.29.0",
        "globals": "^15.11.0",
        "jsdoc": "^4.0.3",
        "npm-run-all2": "^6.2.3",
        "prettier": "^3.3.3",
        "vitepress": "1.4.1"
    }
