{
    "name": "brunfilm",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vitepress build",
        "dev": "vitepress dev --port=4000",
        "eslint": "eslint ",
        "eslint:fix": "eslint --fix",
        "lint": "run-s eslint prettier",
        "lint:fix": "run-s eslint:fix prettier:fix",
        "prepare": "husky",
        "prettier": "npx prettier ./**/* --ignore-unknown --check",
        "prettier:fix": "npx prettier . --ignore-unknown --write",
        "preview": "vitepress preview",
        "test": "npx vitest run --",
        "test:coverage": "npx vitest run --coverage",
        "test:coverage:junit": "vitest run --coverage --reporter=junit --",
        "test:junit": "vitest --reporter=junit",
        "test:run": "vitest run",
        "test:ui": "npx vitest --ui --coverage.enabled=true"
    },
    "devDependencies": {
        "@codecov/vite-plugin": "^1.2.1",
        "@eslint/js": "^9.13.0",
        "@eslint/markdown": "^6.2.1",
        "@vitest/coverage-v8": "^2.1.3",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-vue": "^9.29.1",
        "globals": "^15.11.0",
        "husky": "^9.1.6",
        "iconify-icon": "^2.1.0",
        "jsdoc": "^4.0.4",
        "lint-staged": "^15.2.10",
        "npm-run-all2": "^7.0.1",
        "vitepress": "1.4.1"
    }
}
