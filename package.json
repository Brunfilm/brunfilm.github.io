{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "brunfilm",
    "version": "0.0.1",
    "private": true,
    "description": "",
    "keywords": [
        "brunfilm",
        "vitepress",
        "vite",
        "typescript",
        "vue",
        "javascript"
    ],
    "bugs": {
        "url": "https://github.com/Brunfilm/brunfilm.github.io/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Brunfilm/brunfilm.github.io.git"
    },
    "license": "MIT",
    "author": "Brunfilm",
    "type": "module",
    "scripts": {
        "prebuild": "run-s clean:dist",
        "build": "vitepress build src",
        "postbuild": "run-s tsc",
        "check:eslint": "run-s eslint",
        "check:markdownlint": "run-s markdownlint",
        "check:prettier": "run-s prettier -- --check",
        "check:stylelint": "run-s stylelint",
        "clean": "run-p clean:*",
        "clean:dist": "rimraf src/.vitepress/dist/",
        "dev": "vitepress dev src --port=4000 --mode development",
        "dev:build": "run-s build -- --mode development",
        "eslint": "npx eslint --config eslint.config.mts",
        "eslint:conf": "run-s eslint -- --print-config",
        "eslint:debug": "run-s eslint -- --debug",
        "eslint:env": "run-s eslint --env-info",
        "eslint:inspect": "npx @eslint/config-inspector@latest --config eslint.config.mts",
        "eslint:stats": "run-s eslint --stats",
        "fix:eslint": "run-s eslint -- --fix",
        "fix:markdownlint": "run-s markdownlint",
        "fix:prettier": "run-s prettier -- --write",
        "fix:stylelint": "run-s stylelint -- --fix",
        "lint": "run-s eslint prettier stylelint",
        "lint:check": "run-s check:*",
        "lint:fix": "run-s fix:*",
        "markdownlint": "markdownlint \"**/*.md\" --config=.markdownlint.json --ignore-path=.markdownlintignore",
        "prettier": "prettier --ignore-unknown ./src/**/*.{js,ts,mjs,mts,json}",
        "preview": "vitepress preview src",
        "prod:build": "run-s build -- --mode production",
        "run:tsc": "run-s tsc",
        "preserve": "run-s prod:build",
        "serve": "vitepress serve src",
        "stylelint": "stylelint \"**/*.{styl,vue}\" \"src/.vitepress/**/*.{styl,vue}\"",
        "test": "npx vitest run",
        "test:coverage": "vitest run --coverage.enabled --coverage.clean",
        "test:coverage:clover": "npm run test:coverage -- --coverage.reporter=clover",
        "test:coverage:cobertura": "npm run test:coverage -- --coverage.reporter=cobertura",
        "test:coverage:html": "npm run test:coverage -- --coverage.reporter=html",
        "test:coverage:json": "npm run test:coverage -- --coverage.reporter=json",
        "test:coverage:text": "npm run test:coverage -- --coverage.reporter=text",
        "test:junit": "vitest --reporter=junit",
        "test:run": "vitest run",
        "test:ui": "npx vitest --ui --coverage.enabled",
        "tsc": "vue-tsc --project tsconfig.json"
    },
    "simple-git-hooks": {
        "pre-commit": "npm lint-staged"
    },
    "lint-staged": {
        "*.{html,json}": "prettier --write",
        "*.{styl,vue}": "stylelint --fix"
    },
    "devDependencies": {
        "@eslint/js": "9.20.0",
        "@eslint/json": "0.10.0",
        "@eslint/markdown": "6.2.2",
        "@types/node": "22.13.4",
        "@typescript-eslint/eslint-plugin": "8.24.1",
        "@typescript-eslint/parser": "8.24.1",
        "@vitest/coverage-v8": "3.0.5",
        "@vitest/eslint-plugin": "1.1.31",
        "@vue/tsconfig": "0.7.0",
        "autoprefixer": "10.4.20",
        "eslint": "9.20.1",
        "eslint-config-prettier": "10.0.1",
        "eslint-plugin-chai-friendly": "1.0.1",
        "eslint-plugin-cypress": "4.1.0",
        "eslint-plugin-markdownlint": "0.6.0",
        "eslint-plugin-mocha": "10.5.0",
        "eslint-plugin-oxlint": "0.15.11",
        "eslint-plugin-prettier": "5.2.3",
        "eslint-plugin-vue": "9.32.0",
        "globals": "15.15.0",
        "iconify-icon": "2.3.0",
        "jiti": "2.4.2",
        "jsdoc": "4.0.4",
        "lint-staged": "15.4.3",
        "markdownlint": "0.37.4",
        "markdownlint-cli": "0.44.0",
        "npm-run-all2": "7.0.2",
        "prettier-plugin-vitepress": "0.0.8",
        "rimraf": "6.0.1",
        "stylelint": "16.14.1",
        "typescript": "5.7.3",
        "typescript-eslint": "8.24.1",
        "vite": "6.1.0",
        "vite-plugin-eslint": "1.8.1",
        "vitepress": "1.6.3",
        "vue-eslint-parser": "9.4.3",
        "vue-tsc": "2.2.2"
    },
    "peerDependencies": {
        "eslint": ">=9",
        "vue": ">=3.5"
    },
    "engines": {
        "node": ">=22",
        "npm": ">=10"
    }
}
