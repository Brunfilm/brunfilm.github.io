{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "brunfilm",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "prebuild": "vue-tsc --noEmit",
        "build": "vitepress build --mode -- ",
        "build:dev": "run-s build -- development",
        "build:prod": "run-s build -- production",
        "postbuild": "vue-tsc --declaration --emitDeclarationOnly",
        "dev": "vitepress dev --port=4000 --mode development",
        "eslint": "npx eslint --config eslint.config.mjs",
        "eslint:conf": "npx eslint --print-config",
        "eslint:conf:inspect": "npx @eslint/config-inspector@latest --config eslint.config.mjs",
        "eslint:debug": "npx eslint --debug",
        "eslint:env": "npx eslint --env-info",
        "eslint:fix": "npx eslint --fix",
        "eslint:stats": "npx eslint --stats",
        "lint": "run-s eslint prettier",
        "lint:fix": "run-s eslint:fix prettier:fix",
        "prepare": "",
        "prettier": "npx prettier ./**/* --ignore-unknown --check",
        "prettier:fix": "npx prettier . --ignore-unknown --write",
        "preview": "vitepress preview",
        "serve": "vitepress serve",
        "test": "npx vitest run",
        "test:coverage": "vitest run --coverage.enabled --coverage.clean",
        "test:coverage:clover": "npm run test:coverage -- --coverage.reporter=clover",
        "test:coverage:cobertura": "npm run test:coverage -- --coverage.reporter=cobertura",
        "test:coverage:html": "npm run test:coverage -- --coverage.reporter=html",
        "test:coverage:json": "npm run test:coverage -- --coverage.reporter=json",
        "test:coverage:text": "npm run test:coverage -- --coverage.reporter=text",
        "test:junit": "vitest --reporter=junit",
        "test:run": "vitest run",
        "test:ui": "npx vitest --ui --coverage.enabled",
        "tsc": "tsc"
    },
    "dependencies": {
        "vitepress": "1.5.0"
    },
    "devDependencies": {
        "@eslint/js": "9.18.0",
        "@eslint/json": "0.9.1",
        "@eslint/markdown": "6.2.1",
        "@types/node": "22.10.7",
        "@vitest/coverage-v8": "2.1.8",
        "@vitest/eslint-plugin": "1.1.25",
        "eslint": "9.18.0",
        "eslint-config-prettier": "10.0.1",
        "eslint-plugin-chai-friendly": "1.0.1",
        "eslint-plugin-cypress": "4.1.0",
        "eslint-plugin-markdownlint": "0.6.0",
        "eslint-plugin-mocha": "10.5.0",
        "eslint-plugin-oxlint": "0.15.6",
        "eslint-plugin-prettier": "5.2.2",
        "eslint-plugin-vue": "9.32.0",
        "globals": "15.14.0",
        "iconify-icon": "2.3.0",
        "jsdoc": "4.0.4",
        "lint-staged": "15.4.1",
        "npm-run-all2": "7.0.2",
        "prettier-plugin-vitepress": "0.0.8",
        "typescript": "5.7.3",
        "typescript-eslint": "8.20.0",
        "vue-tsc": "2.2.0"
    },
    "peerDependencies": {
        "eslint": "^9.0.0"
    }
}
